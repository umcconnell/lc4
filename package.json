{
    "name": "lc4",
    "version": "1.1.1",
    "description": "A spec-compliant lc4 encryption/decryption library",
    "main": "./lib/main.js",
    "scripts": {
        "build": "./node_modules/.bin/babel src -s --no-comments --out-dir lib",
        "prepare": "npm run build",
        "test": "./node_modules/.bin/mocha --require @babel/register test/test.js",
        "docs": "jsdoc2md src/main.js > docs/docs.md && jsdoc2md src/*.js > docs/dev.md"
    },
    "keywords": [
        "lc4",
        "encryption",
        "elsiefour"
    ],
    "author": "Ulysse McConnell",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/umcconnell/lc4.git"
    },
    "devDependencies": {
        "@babel/cli": "^7.4.4",
        "@babel/core": "^7.4.5",
        "@babel/preset-env": "^7.4.5",
        "@babel/register": "^7.4.4",
        "babel-preset-minify": "^0.5.0",
        "chai": "^4.2.0",
        "jsdoc-to-markdown": "^5.0.0",
        "mocha": "^6.1.4"
    }
}
